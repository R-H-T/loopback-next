{
  "name": "@loopback/metadata",
  "version": "0.9.12",
  "description": "LoopBack's metadata utilities for reflection and decoration",
  "engines": {
    "node": ">=8.9"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "acceptance": "lb-mocha \"DIST/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-metadata*.tgz dist* package api-docs",
    "pretest": "npm run build",
    "test": "lb-mocha \"DIST/__tests__/unit/**/*.js\" \"DIST/__tests__/acceptance/**/*.js\"",
    "unit": "lb-mocha \"DIST/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-metadata*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.0.1",
    "lodash": "^4.17.5",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@loopback/build": "^0.7.4",
    "@loopback/dist-util": "^0.3.7",
    "@loopback/testlab": "^0.14.1",
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.106",
    "@types/node": "^10.11.2"
  },
  "keywords": [
    "LoopBack",
    "Decorators",
    "Reflect"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
