{
  "name": "@loopback/openapi-v3",
  "version": "0.15.2",
  "description": "Processes openapi v3 related metadata",
  "engines": {
    "node": ">=8.9"
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@loopback/build": "^0.7.4",
    "@loopback/openapi-spec-builder": "^0.9.3",
    "@loopback/repository": "^0.19.0",
    "@loopback/testlab": "^0.14.1",
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.106",
    "@types/node": "^10.11.2"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "build:resources": "lb-copy-resources",
    "clean": "lb-clean loopback-openapi-v3*.tgz dist* package",
    "integration": "lb-mocha \"DIST/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"DIST/__tests__/unit/**/*.js\" \"DIST/__tests__/integration/**/*.js\"",
    "unit": "lb-mocha \"DIST/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-openapi-v3*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "Swagger",
    "OpenAPI Spec",
    "TypeScript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  },
  "dependencies": {
    "@loopback/context": "^0.12.13",
    "@loopback/dist-util": "^0.3.7",
    "@loopback/openapi-v3-types": "^0.9.5",
    "@loopback/repository-json-schema": "^0.11.2",
    "debug": "^4.0.1",
    "lodash": "^4.17.5"
  }
}
